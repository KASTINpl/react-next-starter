{
  "name": "react-next-starter",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "plop": "plop",
    "test": "run-p test:*",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:unit:update": "jest --updateSnapshot",
    "test:e2e": "cypress run",
    "test:e2e:ui": "cypress open",
    "lint": "run-p lint:*",
    "lint:types": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:format": "yarn prettier --check --ignore-unknown .",
    "storybook": "storybook dev -c src/storybook/config -p 6006",
    "build-storybook": "storybook build -c src/storybook/config",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@fontsource/roboto": "5.1.1",
    "@mui/icons-material": "5.14.19",
    "@mui/material": "5.14.19",
    "@reduxjs/toolkit": "1.9.7",
    "clsx": "2.1.1",
    "next": "13.5.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "redux-devtools": "3.7.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "7.6.20",
    "@storybook/addon-essentials": "7.6.20",
    "@storybook/addon-links": "7.6.20",
    "@storybook/addon-mdx-gfm": "7.6.20",
    "@storybook/nextjs": "7.6.20",
    "@storybook/react": "7.6.20",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "14.3.1",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.6.0",
    "@types/jest": "29.5.14",
    "@types/node": "18.19.71",
    "@types/react": "18.2.41",
    "@types/react-dom": "18.2.17",
    "@types/react-redux": "7.1.34",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "babel-jest": "29.7.0",
    "babel-plugin-module-resolver": "5.0.2",
    "cypress": "12.17.4",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.5.8",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "14.0.1",
    "npm-run-all": "4.1.5",
    "plop": "3.1.2",
    "prettier": "3.1.0",
    "storybook": "7.6.20",
